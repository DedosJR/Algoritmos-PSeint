Algoritmo While 
	Definir num,i, fact Como Entero;
	Escribir " Capture Numero "; 
	Leer num; 
	i=1;
	fact=1;
	mientras i<=num  Hacer
		fact=fact*i;
		i=i+1;
		
	FinMientras
	
	Escribir " El Factorial de ", num, " es: ", fact;
FinAlgoritmo

