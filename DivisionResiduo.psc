Algoritmo divisionResiduo
	Definir num1, num2, cociente, modulo Como Real;
	Escribir "Ingrese el dividendo";
	Leer num1;
	Escribir "Ingrese el dividendo";
	Leer num2;
	cociente=TRUNC(num1/num2);
	modulo=num1 % num2;
	Escribir "El resultado es; ", cociente;
	Escribir "El resultado es; ", modulo;
	
FinAlgoritmo
